{
  "name": "holodeck",
  "version": "0.0.1",
  "description": "A set of styled-components with easily editable themes.",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel ./src -d ./lib",
    "bundle": "",
    "lint": "./node_modules/.bin/eslint ./src --ext .js,.jsx",
    "lintfix": "./node_modules/.bin/eslint ./src --ext .js,.jsx --fix",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha --require babel-core/register",
    "storybook": "",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Freedombase/holodeck.git"
  },
  "keywords": [
    "styled-components"
  ],
  "author": "Jan Dvorak <storyteller@freedombase.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Freedombase/holodeck/issues"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "homepage": "https://github.com/Freedombase/holodeck#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1"
  }
